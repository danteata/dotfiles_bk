# some more ls aliases
alias dantheta='echo THE NAME OF THE LORD IS A STRONG TOWER.'
alias railsy='bundle exec rails s'
alias go='foreman start'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias pour='ls -lah'
alias get='sudo apt-get install'
alias nobashish='BASHISH_DISABLED=1'
alias bashish='BASHISH_DISABLED=0'
alias dbx='./dropbox.py'
alias migrate='rake db:migrate'
alias railspf='cd ~/rails_projects & ls'
alias sproutcorepf='cd ~/sproutcore_projects & ls'
alias dbxfolder='cd /media/Data/My\ Dropbox/Dropbox'
